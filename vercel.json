{
    "rewrites": [
        {
            "source": "/admin/analytics",
            "destination": "/admin/analytics.html"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/|admin/analytics.html|styles.css|script.js|logo.png|favicon.ico).*)",
            "destination": "/index.html"
        }
    ]
}